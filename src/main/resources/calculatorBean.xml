<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
        "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean id="toyota" class="aint.electronics.Toyota" autowire="constructor">
    </bean>

    <bean id = "carModelType" class="aint.electronics.Car">
        <property name="model" value="lexus"/>
    </bean>

    <bean id = "carMakeYear" class="aint.electronics.Car">
        <property name="year" value="2019"/>
    </bean>

</beans>
